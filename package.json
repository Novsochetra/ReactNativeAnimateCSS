{
  "name": "react-native-animated-css",
  "version": "0.0.1",
  "description": "donut chart for react-native.",
  "main": "index.js",
  "module": "index.js",
  "react-native": "index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "yarn build",
    "prepare": "bob build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novsochetra/ReactNativeAnimateCSS.git"
  },
  "keywords": [
    "animation",
    "animated",
    "animated css",
    "react-native-animated-css",
    "react native animated css",
    "react-native"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.64.0",
    "react-native-reanimated": ">=2.2.0"
  },
  "author": "sochetra NOV <sochetra12.nov@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Novsochetra/ReactNativeAnimateCSS/issues"
  },
  "devDependencies": {
    "@types/react-native": "^0.64.5",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "hygen": "^6.1.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.2"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
